{
  "name": "booklist",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "vercel": "next build",
    "start": "next start",
    "lint": "next lint",
    "commit": "git pull && bun update && bun run vercel && read -p 'Descriptions Commit: ' message && git add . && echo -n \"$message - \" && node getTime.js | xargs -I {} git commit -m \"$message - {}\" && git push"
  },
  "dependencies": {
    "firebase": "^11.4.0",
    "next": "15.2.1",
    "next-seo": "^6.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.1",
    "styled-components": "^6.1.15"
  },
  "devDependencies": {
    "typescript": "^5.8.2",
    "@types/node": "^20.17.24",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4"
  }
}
